Attribute VB_Name = "d2set"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Private Sub db2_Click()
    Dim i As Integer
    
    For i = 0 To 20
        SenderID(i).Enabled = "1"
        APidID(i).Enabled = "0"
    Next i
End Sub
Private Sub dreamb_Click()
    Dim i As Integer
    
    For i = 0 To 20
        SenderID(i).Enabled = "0"
        APidID(i).Enabled = "1"
    Next i
End Sub
Private Sub Form_unLoad(cancel As Integer)
    cancel = 1
    Call OK_Click
End Sub
Public Sub OK_Click()
    Dim temp As Integer, i As Integer, counter(1 To 3) As Integer
    
    For i = 0 To 20
        If d2set.T1(i).Value = "1" Then counter(1) = counter(1) + 1
        If d2set.T2(i).Value = "1" Then counter(2) = counter(2) + 1
        If d2set.T3(i).Value = "1" Then counter(3) = counter(3) + 1
    Next i
        
    If counter(1) > 8 Or counter(2) > 8 Or counter(3) > 8 Then
        MsgBox "No more than 8 channels are allowed for each Multichannel!"
        Exit Sub
    End If
        
    If db2.Value = "1" Then
        temp = frmMain.senderauswahl.ListIndex
        frmMain.senderauswahl.Clear
        
        For i = 0 To 20
            frmMain.senderauswahl.AddItem d2set.SenderID(i).Text
        Next i
        
        frmMain.senderauswahl.AddItem "Multi-channel 1"
        frmMain.senderauswahl.AddItem "Multi-channel 2"
        frmMain.senderauswahl.AddItem "Multi-channel 3"
        
        frmMain.senderauswahl.ListIndex = temp
    Else
        temp = frmMain.senderauswahl.ListIndex
        frmMain.senderauswahl.Clear
            
        For i = 0 To 20
            frmMain.senderauswahl.AddItem d2set.APidID(i).Text + " - " + plbezeichnung.plID(i).Text
        Next i
        
        If temp > 20 Then
            frmMain.senderauswahl.ListIndex = 0
        Else
            frmMain.senderauswahl.ListIndex = temp
        End If
    End If
    
    d2set.Hide
    frmMain.Show
End Sub
Private Sub ping_Click()
    If Werkzeuge.ping(d2set.IP.Text) Then
        MsgBox "Connection OK"
    Else
        MsgBox "Connection failed"
    End If
End Sub
